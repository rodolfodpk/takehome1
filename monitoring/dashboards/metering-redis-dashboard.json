        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "decimals": 4
          }
        }
      },
      {
        "id": 4,
        "title": "Events Stored per Second",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(metering_redis_storage_latency_seconds_count[1m])",
