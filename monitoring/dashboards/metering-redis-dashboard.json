{
  "dashboard": {
    "title": "Metering - Redis Metrics",
    "tags": ["metering", "redis", "technical"],
    "timezone": "browser",
    "refresh": "10s",
    "time": {
      "from": "now-15m",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "Redis Storage Latency",
        "type": "timeseries",
        "targets": [
          {
            "expr": "metering_redis_storage_latency_seconds{quantile=\"0.50\"}",
            "legendFormat": "P50",
            "refId": "A"
          },
          {
            "expr": "metering_redis_storage_latency_seconds{quantile=\"0.95\"}",
            "legendFormat": "P95",
            "refId": "B"
          },
          {
            "expr": "metering_redis_storage_latency_seconds{quantile=\"0.99\"}",
            "legendFormat": "P99",
            "refId": "C"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "decimals": 4,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 0.01, "color": "yellow"},
                {"value": 0.02, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 2,
        "title": "Redis Counter Update Latency",
        "type": "timeseries",
        "targets": [
          {
            "expr": "metering_redis_counter_update_latency_seconds{quantile=\"0.50\"}",
            "legendFormat": "P50",
            "refId": "A"
          },
          {
            "expr": "metering_redis_counter_update_latency_seconds{quantile=\"0.95\"}",
            "legendFormat": "P95",
            "refId": "B"
          },
          {
            "expr": "metering_redis_counter_update_latency_seconds{quantile=\"0.99\"}",
            "legendFormat": "P99",
            "refId": "C"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "decimals": 4
          }
        }
      },
      {
        "id": 3,
        "title": "Redis Read Latency",
        "type": "timeseries",
        "targets": [
          {
            "expr": "metering_redis_read_latency_seconds{quantile=\"0.50\"}",
            "legendFormat": "P50",
            "refId": "A"
          },
          {
            "expr": "metering_redis_read_latency_seconds{quantile=\"0.95\"}",
            "legendFormat": "P95",
