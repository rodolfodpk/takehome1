{
  "title": "Metering - Business Metrics",
  "tags": [
    "metering",
    "business",
    "events"
  ],
  "timezone": "browser",
  "refresh": "30s",
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "panels": [
    {
      "id": 1,
      "title": "Events Ingested per Second (Total)",
      "type": "timeseries",
      "targets": [
        {
          "expr": "rate(metering_events_ingested_total{type=\"total\"}[1m])",
          "legendFormat": "Events/sec",
          "refId": "A"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 0
      },
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "decimals": 2
        }
      }
    },
    {
      "id": 2,
      "title": "Events Ingested by Tenant (Top 10)",
      "type": "timeseries",
      "targets": [
        {
          "expr": "topk(10, rate(metering_events_ingested_total{type=\"by_tenant\"}[1m]))",
          "legendFormat": "Tenant {{tenant_id}}",
          "refId": "A"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 0
      },
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "decimals": 2
        }
      }
    },
    {
      "id": 3,
      "title": "Events Ingested by Customer (Top 10)",
      "type": "timeseries",
      "targets": [
        {
          "expr": "topk(10, rate(metering_events_ingested_total{type=\"by_customer\"}[1m]))",
          "legendFormat": "Customer {{customer_id}} (Tenant {{tenant_id}})",
          "refId": "A"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 8
      },
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "decimals": 2
        }
      }
    },
    {
      "id": 4,
      "title": "Ingestion Errors per Second",
      "type": "timeseries",
      "targets": [
        {
          "expr": "rate(metering_events_ingestion_errors_total[1m])",
          "legendFormat": "Errors/sec",
          "refId": "A"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 8
      },
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "decimals": 2,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": 0,
                "color": "green"
              },
              {
                "value": 1,
                "color": "yellow"
              },
              {
                "value": 10,
                "color": "red"
              }
            ]
          }
        }
      }
    },
    {
      "id": 5,
      "title": "Event Processing Latency",
      "type": "timeseries",
      "targets": [
        {
          "expr": "metering_events_processing_latency_seconds{quantile=\"0.50\"}",
          "legendFormat": "P50",
          "refId": "A"
        },
        {
          "expr": "metering_events_processing_latency_seconds{quantile=\"0.95\"}",
          "legendFormat": "P95",
          "refId": "B"
        },
        {
          "expr": "metering_events_processing_latency_seconds{quantile=\"0.99\"}",
          "legendFormat": "P99",
          "refId": "C"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 16
      },
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "decimals": 3,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": 0,
                "color": "green"
              },
              {
                "value": 0.1,
                "color": "yellow"
              },
              {
                "value": 0.2,
                "color": "red"
              }
            ]
          }
        }
      }
    },
    {
      "id": 6,
      "title": "Ingestion Error Rate %",
      "type": "timeseries",
      "targets": [
        {
          "expr": "(rate(metering_events_ingestion_errors_total[1m]) / (rate(metering_events_ingested_total{type=\"total\"}[1m]) + vector(0.0001))) * 100",
          "legendFormat": "Error Rate %",
          "refId": "A"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 16
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "decimals": 3,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": 0,
                "color": "green"
              },
              {
                "value": 0.1,
                "color": "yellow"
              },
              {
                "value": 1,
                "color": "red"
              }
            ]
          }
        }
      }
    },
